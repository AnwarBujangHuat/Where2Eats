{
  "compilerOptions": {
    "baseUrl": "./app",
    "paths": {
      "images": [
        "./assets/images"
      ],
      "lottie": [
        "./assets/lottie"
      ],
      "atoms": [
        "./components/atoms"
      ],
      "molecules": [
        "./components/molecules"
      ],
      "config": [
        "./config"
      ],
      "model": [
        "./model"
      ],
      "navigation": [
        "./navigation"
      ],
      "screens": [
        "./screens"
      ],
      "store": [
        "./store"
      ],
      "assets": [
        "./assets"
      ]
    },
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "es2017"
    ],
    "filesGlob": [
      "*.ts",
      "*.tsx"
    ]
  },
  "include": [
    "src/global.d.ts",
    "src"
  ]
}
